        {:des   "left_shift to ("
         :rules [[:##left_shift :left_shift nil {:alone :!S9}]]}
        {:des   "right_shift to )"
         :rules [[:##right_shift :right_shift nil {:alone :!S0}]]}
        {:des   "caps_lock to control and escape"
         :rules [[:##caps_lock :left_control nil {:alone :escape}]]}
        {:des   "left_control to control and escape"
         :rules [[:##left_control :left_control nil {:alone :escape}]]}
