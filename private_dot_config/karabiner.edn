                 [:##l :right_arrow ["layer" 3]]
                 [:##semicolon :hyphen ["layer" 3]]
                 [:##quote :equal_sign ["layer" 3]]
                 [:##z :!Sopen_bracket ["layer" 3]]
                 [:##x :7 ["layer" 3]]
                 [:##c :8 ["layer" 3]]
                 [:##v :9 ["layer" 3]]
                 [:##b :!S3 ["layer" 3]]
                 [:##n :home ["layer" 3]]
                 [:##m :page_down ["layer" 3]]
                 [:##comma :page_up ["layer" 3]]
                 [:##period :end ["layer" 3]]
                 [:##slash :!Sclose_bracket ["layer" 3]]
                 [:##left_shift :left_shift ["layer" 3] {:alone :open_bracket}]
                 [:##right_shift :right_shift ["layer" 3] {:alone :close_bracket}]
                 ]}
