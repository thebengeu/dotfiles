{:profiles {:Default {:default true
                      :alone   150
                      :delay   500
                      ; $ defaults read -g InitialKeyRepeat
                      :held    225
                      :sim     50}}
 :main [{:des "Layer 3"
         :rules [[:##tab :!Sgrave_accent_and_tilde ["layer" 3]]
                 [:##q :!S1 ["layer" 3]]
                 [:##w :1 ["layer" 3]]
                 [:##e :2 ["layer" 3]]
                 [:##r :3 ["layer" 3]]
                 [:##t :!S5 ["layer" 3]]
                 [:##y :!S6 ["layer" 3]]
                 [:##u :!S7 ["layer" 3]]
                 [:##i :!S8 ["layer" 3]]
                 [:##o :!S4 ["layer" 3]]
                 [:##p :!Shyphen ["layer" 3]]
                 [:##open_bracket :!Sequal_sign ["layer" 3]]
                 [:##caps_lock :left_control ["layer" 3] {:alone :grave_accent_and_tilde}]
                 [:##left_control :left_control ["layer" 3] {:alone :grave_accent_and_tilde}]
                 [:##a :!S2 ["layer" 3]]
                 [:##s :4 ["layer" 3]]
                 [:##d :5 ["layer" 3]]
                 [:##f :6 ["layer" 3]]
                 [:##g :0 ["layer" 3]]
